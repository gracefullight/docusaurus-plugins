{
  "name": "docu-plugins",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "^2.8.3",
    "turbo": "^1.7.0"
  },
  "engines": {
    "node": "18"
  },
  "packageManager": "yarn@3.3.1"
}
